---
layout: false
---
<!DOCTYPE html>
<html lang="<%= config.site.language %>">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= config.site.title %>
    </title>
    <%- css('asset/styles/index') %>
</head>

<body>
    <div class="code-list">
        <% page.codes.forEach((code)=>{ %>
            <div class="code-item">
                <div class="code-ifr">
                    <a href="<%= url_format(code.route_url)+'editor' %>" class="btn-editor" target="_blank">在线编辑</a>
                    <iframe src="<%= url_format(code.route_url) %>" sandbox="allow-scripts allow-same-origin" scrolling='no'></iframe>
                </div>
                <div class="cover-info">
                    <h4>
                        <a href="<%= url_format(code.route_url) %>" target="_blank">
                            <%= code.title %>
                        </a>
                    </h4>
                    <small>
                        <%= code.description %>
                    </small>
                </div>
            </div>
            <% }) %>
    </div>
    <%- paginator() %>
</body>

</html>